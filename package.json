{
  "name": "stegjs",
  "version": "2.0.3",
  "description": "Encrypt message to PNG image.",
  "main": "lib/index.js",
  "types": "lib",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "build": "rimraf ./lib && tsc -p . --sourcemap",
    "prettier-format": "run-script-os",
    "prettier-format:win32": "prettier --config .prettierrc \"./src/**/*.ts\" --write",
    "prettier-format:darwin:linux": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-format:default": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-watch": "run-script-os",
    "prettier-watch:win32": "onchange \"src/**/*.ts\" -- prettier --write {{changed}}",
    "prettier-watch:darwin:linux": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "prettier-watch:default": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "test": "jest --coverage",
    "test:dev": "jest --watchAll --coverage --detectOpenHandles"
  },
  "bin": {
    "stegjs": "lib/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run prettier-format && npm run lint"
    }
  },
  "keywords": [
    "steganography",
    "image",
    "hide",
    "secret",
    "array",
    "encode",
    "decode",
    "least",
    "significant",
    "bit"
  ],
  "author": "Andrey Medvedev <a.medvedev@me.com> (https://github.com/andmev/)",
  "files": [
    "bin/",
    "app/",
    "lib/",
    "dist/",
    "src/",
    "resources/",
    "menu/",
    "scripts/",
    "build/",
    "cli.js",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/andmev/stegjs.git"
  },
  "bugs": {
    "url": "https://github.com/andmev/stegjs/issues"
  },
  "homepage": "https://github.com/andmev/stegjs#readme",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^8.1.0",
    "eslint-plugin-jest": "^24.4.0",
    "jest": "^27.0.6",
    "node-fetch": "^3.1.1",
    "pngjs": "^6.0.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.6.1",
    "@types/node-fetch": "^2.5.12",
    "@types/pngjs": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "onchange": "^7.1.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6",
    "ts-node": "^10.2.0"
  }
}
